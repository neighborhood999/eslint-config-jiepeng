{
  "private": true,
  "name": "eslint-config-jiepeng",
  "description": "neighborhood999's eslint config workspace.",
  "license": "MIT",
  "author": "Peng Jie <im@jiepeng.me>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neighborhood999/eslint-config-jiepeng.git"
  },
  "bugs": {
    "url": "https://github.com/neighborhood999/eslint-config-jiepeng/issues"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "changeset": "changeset",
    "postinstall": "manypkg check",
    "lint": "eslint 'packages/**/*.js'",
    "lint:fix": "npm run lint -- --fix",
    "release": "changeset publish"
  },
  "dependencies": {
    "@changesets/changelog-github": "^0.4.0",
    "@changesets/cli": "^2.16.0",
    "@manypkg/cli": "^0.17.0",
    "babel-eslint": "^10.1.0",
    "confusing-browser-globals": "^1.0.10",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "eslint-plugin-unicorn": "^29.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.2.1"
  },
  "keywords": [
    "config",
    "eslint",
    "eslintconfig",
    "javascript",
    "lint",
    "prettier",
    "react"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "yarn lint:fix"
    ]
  }
}
