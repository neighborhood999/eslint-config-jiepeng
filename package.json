{
  "name": "eslint-config-jiepeng",
  "private": true,
  "description": "neighborhood999's eslint config workspace.",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "lint": "eslint 'packages/**/*.js'",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "manypkg check",
    "changeset": "changeset",
    "release": "changeset publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neighborhood999/eslint-config-jiepeng.git"
  },
  "bugs": {
    "url": "https://github.com/neighborhood999/eslint-config-jiepeng/issues"
  },
  "author": "Peng Jie <im@jiepeng.me>",
  "keywords": [
    "javascript",
    "eslint",
    "eslintconfig",
    "prettier",
    "lint",
    "config",
    "react"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "yarn lint:fix"
    ]
  },
  "dependencies": {
    "@changesets/changelog-github": "^0.2.6",
    "@changesets/cli": "^2.9.1",
    "@manypkg/cli": "^0.14.0",
    "babel-eslint": "^10.1.0",
    "confusing-browser-globals": "^1.0.9",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "eslint-plugin-unicorn": "^20.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "license": "MIT"
}
