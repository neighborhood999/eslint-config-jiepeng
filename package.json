{
  "private": true,
  "name": "eslint-config-jiepeng",
  "description": "neighborhood999's eslint config workspace.",
  "license": "MIT",
  "author": "Peng Jie <im@jiepeng.me>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neighborhood999/eslint-config-jiepeng.git"
  },
  "bugs": {
    "url": "https://github.com/neighborhood999/eslint-config-jiepeng/issues"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "changeset": "changeset",
    "postinstall": "manypkg check",
    "lint": "eslint 'packages/**/*.js'",
    "lint:fix": "npm run lint -- --fix",
    "release": "changeset publish",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/core": "^7.14.8",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/eslint-plugin": "^7.14.5",
    "@changesets/changelog-github": "^0.4.0",
    "@changesets/cli": "^2.16.0",
    "@manypkg/cli": "^0.18.0",
    "babel-eslint": "^10.1.0",
    "confusing-browser-globals": "^1.0.10",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-unicorn": "^33.0.1",
    "husky": "^7.0.4",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2"
  },
  "keywords": [
    "config",
    "eslint",
    "eslintconfig",
    "javascript",
    "lint",
    "prettier",
    "react"
  ],
  "lint-staged": {
    "**/*.js": [
      "yarn lint:fix"
    ]
  }
}
